{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${env:GODOT_PATH}"
        },
        {
            "name": "Launch Scene",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${env:GODOT_PATH}",
            "args": [
                "${command:godot.csharp.getLaunchScene}"
            ]
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
